<h3>以下の内容で、投稿する。</h3>
<%= form_with(model: @picture, url: pictures_path, local: true) do |form| %>
  <%= form.hidden_field :content %>
  <%= form.hidden_field :image %>
  <p>タイトル:<%= @picture.content %></p>
  <p>本文:<%= @picture.image %></p>
  <%= form.submit  "投稿する" %>
  <%= form.submit "戻る", name: 'back' %>
<% end %>